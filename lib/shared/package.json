{
  "private": true,
  "name": "@lib/shared",
  "version": "0.0.1",
  "author": "Elias Mawa <elias@mawa.dev>",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./enums": "./dist/cjs/index.js",
    "./helpers": "./dist/cjs/index.js",
    "./models": "./dist/cjs/index.js",
    "./schema": "./dist/cjs/index.js"
  },
  "typesVersions": {
    "*": {
      "enums": [
        "./dist/cjs/enums/index.d.ts"
      ],
      "helpers": [
        "./dist/cjs/helpers/index.d.ts"
      ],
      "models": [
        "./dist/cjs/models/index.d.ts"
      ],
      "schema": [
        "./dist/cjs/schema/index.d.ts"
      ]
    }
  },
  "scripts": {
    "lint": "echo 'lint @lib/shared' && eslint . --ext .ts",
    "prettier": "yarn prettier --write",
    "clean": "",
    "rollup": "echo 'bundle @lib/shared' && rollup -c --bundleConfigAsCjs",
    "rollup:watch": "nodemon --watch 'src/' --delay 200ms -e ts --exec yarn rollup",
    "build": "NODE_ENV=production yarn build:cjs",
    "build:cjs": "echo 'build @lib/shared:cjs' && tsc",
    "build:watch": "nodemon --watch 'src/' -e ts --exec yarn build",
    "test": "NODE_ENV=production jest"
  },
  "dependencies": {
    "@sodaru/yup-to-json-schema": "~2.0.1",
    "date-fns": "~2.28.0",
    "date-fns-tz": "~2.0.0",
    "yup": "~1.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "~15.1.0",
    "@rollup/plugin-terser": "~0.4.3",
    "@rollup/plugin-typescript": "~11.1.2",
    "@types/koa": "~2.13.4",
    "rollup": "~3.23.0",
    "rollup-plugin-dts": "~5.2.0",
    "typescript": "~5.1.6"
  }
}
